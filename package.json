{
  "name": "typescript-template",
  "version": "0.0.0",
  "description": "A typescript template to start from with your next project.",
  "main": "./build/index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,yml,md}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "test:tsc": "tsc --noEmit",
    "dev": "nodemon ./src/index.ts",
    "build": "tsc",
    "start": "node ./build/src/index.js",
    "test": "jest"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Tovade",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tovade/typescript-template.git"
  },
  "bugs": {
    "url": "https://github.com/tovade/typescript-template/issues"
  },
  "homepage": "https://github.com/tovade/typescript-template#readme",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@casper124578/eslint-config": "^6.0.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.5.2",
    "babel-jest": "^29.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3"
  }
}
